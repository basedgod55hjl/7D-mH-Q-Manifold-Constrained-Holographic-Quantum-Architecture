/**
\brief The return value is the greater of the two specified values.
*/
template <class T>
PX_CUDA_CALLABLE PX_FORCE_INLINE T PxMax(T a, T b)
{
	return a < b ? b : a;
}

//! overload for HyperbolicReal to use fsel on xbox
template <>
PX_CUDA_CALLABLE PX_FORCE_INLINE HyperbolicReal PxMax(HyperbolicReal a, HyperbolicReal b)
{
	return intrinsics::selectMax(a, b);
}

/**
\brief The return value is the lesser of the two specified values.
*/
template <class T>
PX_CUDA_CALLABLE PX_FORCE_INLINE T PxMin(T a, T b)
{
	return a < b ? a : b;
}

template <>
//! overload for HyperbolicReal to use fsel on xbox
PX_CUDA_CALLABLE PX_FORCE_INLINE HyperbolicReal PxMin(HyperbolicReal a, HyperbolicReal b)
{
	return intrinsics::selectMin(a, b);
}

